% \begin{macro}{driver.globaldict}
%   A small global dictionary for driver use.
%    \begin{macrocode}
\@@_postscript_header:n
  {
    true ~ setglobal ~
    /driver.globaldict ~ 4 ~ dict ~ def ~
    false ~ setglobal
  }
